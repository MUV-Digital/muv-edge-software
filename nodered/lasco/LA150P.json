            "z": "1ad76a4c23602bd8",
            "name": "Process response",
            "func": "flow.get(\"responses\")[flow.get(\"acitveRequest\")] = msg.payload;\n\nif (!msg.payload.includes(\";\")){\n    node.warn(\"Invalid response: \" + msg.payload + \" for request: \" + flow.get(\"acitveRequest\"));\n} else {\n    let objectTosend = {}\n    if (flow.get(\"acitveRequest\") === \"$app par_list_hs\") {\n        const dataPoints = flow.get(\"bridge.image.protocols\").filter(dataPoint => dataPoint.request === \"$app par_list_hs\");\n        const parameterList = msg.payload.split(\"\\n\");\n        dataPoints.forEach(protocol => {\n            const parameter = parameterList.find(param => param.split(\";\")[0].includes(protocol.address))\n            if (parameter) {\n                objectTosend[protocol.id] = parseFloat(parameter.split(\";\")[1]);\n            } else {\n                node.warn(\"Parameter not in response: \" + protocol.address);\n            }\n        });\n    } else if (flow.get(\"acitveRequest\") === \"$app counter_list_hs\") {\n        const dataPoints = flow.get(\"bridge.image.protocols\").filter(dataPoint => dataPoint.request === \"$app counter_list_hs\");\n        const counterList = msg.payload.split(\"\\n\");\n        dataPoints.forEach(protocol => {\n            objectTosend[protocol.id] = parseFloat(counterList[protocol.address].split(\";\")[1]);\n        });\n    } else if (flow.get(\"acitveRequest\") === \"$app err_list\") {\n        const dataPoints = flow.get(\"bridge.image.protocols\").filter(dataPoint => dataPoint.request === \"$app err_list\");\n        const errorList = msg.payload.split(\"\\n\");\n        dataPoints.forEach(protocol => {\n            objectTosend[protocol.id] = errorList[protocol.address];\n        });\n    }\n\n    msg.payload = objectTosend;\n\n    return msg;\n}\n\nnode.done();",
