        {
            "id": "je7evkzdawmconja",
            "type": "function",
            "z": "ccf63bb125af2871",
            "name": "Set data point values",
            "func": "var bacnetPoints = []\nvar dataPointvalues = {\"payload\" :{}};\n\nfor (var propertyName in msg.payload) {\n    if (propertyName.includes(\"OBJECT_LIST\")){\n        msg.payload[propertyName].map(bacnetObject => {\n            let point = {};\n            Object.keys(bacnetObject).forEach(key => {\n                if (key.includes(\"OBJECT_NAME\")){\n                    point.id = bacnetObject[key];\n                } else if (key.includes(\"PRESENT_VALUE\")) {\n                    point.value = bacnetObject[key];\n                }\n            })\n            if(point.id && point.value){\n                bacnetPoints.push(point);\n            }\n        });\n    }\n}\n\nflow.get(\"bridge.image.protocols\").map(dataPoint => {\n    let bacnetPoint = bacnetPoints.find(element => element.id === dataPoint.id);\n    if (bacnetPoint && dataPoint.topic.includes(\"bool\"))  {\n        dataPointvalues.payload[dataPoint.id] = bacnetPoint.value.includes(\"INACTIVE\") ? false : true;\n    } else if (bacnetPoint) {\n        dataPointvalues.payload[dataPoint.id] = bacnetPoint.value;\n    }\n});\n\nreturn dataPointvalues;",
